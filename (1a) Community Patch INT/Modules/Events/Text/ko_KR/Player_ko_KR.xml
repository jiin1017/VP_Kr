		<!-- Advisor Squabble -->
		<Row Tag="TXT_KEY_PLAYER_EVENT_ADVISOR_DEBATE">
			<Text>특별 회기</Text>
		</Row>
		<Row Tag="TXT_KEY_PLAYER_EVENT_ADVISOR_DEBATE_HELP">
